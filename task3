#include <stdio.h>

int main() {SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    int r;
    printf("Введіть число розрядів (р): ");
    scanf("%d", &r);

    // Ініціалізація початкових значень
    long long dp[r+1][2][2];
    dp[1][0][0] = 1; // однацифрове число, закінченням на 5, немає трьох однакових цифр поруч
    dp[1][1][0] = 1; // однацифрове число, закінченням на 9, немає трьох однакових цифр поруч
    dp[1][0][1] = 0; // однацифрове число, закінченням на 5, однакові цифри поруч
    dp[1][1][1] = 0; // однацифрове число, закінченням на 9, однакові цифри поруч

    // Обчислення кількості чисел
    for (int i = 2; i <= r; i++) {
        dp[i][0][0] = dp[i-1][1][0] + dp[i-1][1][1]; // закінченням на 5, немає трьох однакових цифр поруч
        dp[i][1][0] = dp[i-1][0][0] + dp[i-1][0][1]; // закінченням на 9, немає трьох однакових цифр поруч
        dp[i][0][1] = dp[i-1][0][0]; // закінченням на 5, однакові цифри поруч
        dp[i][1][1] = dp[i-1][1][0]; // закінченням на 9, однакові цифри поруч
    }

    // Виведення результату
    long long count = dp[r][0][0] + dp[r][1][0] + dp[r][0][1] + dp[r][1][1];
    printf("Кількість чисел із %d розрядів: %lld\n", r, count);

    return 0;
}
