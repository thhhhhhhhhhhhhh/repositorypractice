#include <stdio.h>
#include <stdlib.h>
#include <windows.h>

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251); 

    int n, count = 0;
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);

    if (n > 1 && n < 150) { // Перевірка, чи число n є в межах 1 до 150
        for (int i = 1; i < n; i++) {
            if (n % i == n / i) { // Перевірка, чи i є рівним дільником числа n
                count++; // Збільшення лічильника рівних дільників
            }
        }

        printf("Кількість рівних дільників числа %d: %d\n", n, count); // Виведення кількості рівних дільників числа n
    }
    else {
        printf("Невірне значення n. Введіть натуральне число в межах 1 до 150.\n"); // Виведення повідомлення про невірне значення n
    }

    return 0;
}
