#include <stdio.h>
#include <stdlib.h>
#include <windows.h>

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    int n, count = 0;
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);

    if (n > 1 && n < 150) {
        for (int i = 1; i < n; i++) {
            if (n % i == n / i) {
                count++;
            }
        }

        printf("Кількість рівних дільників числа %d: %d\n", n, count);
    }
    else {
        printf("Невірне значення n. Введіть натуральне число в межах 1 до 150.\n");
    }

    return 0;
}

